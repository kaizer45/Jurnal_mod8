public class no1{
	public static void main(String args[]){
		int n = 4;
		int m = 7;
		int x = 0;
		int y = 1;
		int a = 0;
		int b = 1;
		int sum1, sum2;
		sum1 = 0;
		sum2 = 0;
		for(int i = 0; i < n; i++){
			y = x + y;
			x = y - x;
			sum1 = sum1 + x
		}
		System.out.print(sum1);
		System.out.println();
		for(int j = 0; j < m; j++){
			b = a + b;
			a = b - a;
			sum2 = sum2 + a
		}
		System.out.print(sum2);
	}
}
