public class no1{
	public static void main(String args[]){
		int n = 4;
		int m = 7;
		int x = 0;
		int y = 1;
		int a = 0;
		int b = 1;
		for(int i = 0; i < n; i++){
			System.out.print(y);
			System.out.print(", ");
			y = x + y;
			x = y - x;
		}
		System.out.println();
		for(int j = 0; j < m; j++){
			System.out.print(b);
			System.out.print(", ");
			b = a + b;
			a = b - a;
		}
	}
}
